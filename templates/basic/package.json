{
  "name": "<%= name %>",
  "version": "<%= version %>",
  "description": "<%= description %>",
  "scripts": {
    "docker:build": "cross-conf-env docker image build . -t $npm_package_config_imageRepo:$npm_package_version",
    "postdocker:build": "npm run docker:tag",
    "docker:tag": " cross-conf-env docker image tag $npm_package_config_imageRepo:$npm_package_version $npm_package_config_imageRepo:latest",
    "predocker:publish": "echo Attention! Ensure `docker login` is correct.",
    "docker:publish": "cross-conf-env docker image push $npm_package_config_imageRepo:$npm_package_version",
    "postdocker:publish": "cross-conf-env docker image push $npm_package_config_imageRepo:latest",
    "compose:up": "APP_IMAGE=$npm_package_config_imageRepo:$npm_package_version  docker-compose -f front-model-dev.yml up",
    "compose:up-prod": "APP_IMAGE=$npm_package_config_imageRepo:$npm_package_version  docker-compose -f front-model-pro.yml up",
    "compose:down": "docker-compose -f front-model-dev.yml down",
    "compose:down-prod": "docker-compose -f front-model-pro.yml down"
  },
  "author": "<%= author %>",
  "license": "<%= license %>",
  "dependencies": {
    "@quick-qui/data-provider": "^1.1.3",
    "@quick-qui/model-defines": "^2.0.0",
    "@stomp/stompjs": "^5.4.2",
    "@types/lodash": "^4.14.148",
    "@types/uniqid": "^4.1.3",
    "lodash": "^4.17.15",
    "typescript": "^3.7.2",
    "uniqid": "^5.1.0"
  },
  "devDependencies": {}
}
